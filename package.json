{
  "name": "dikmax.name",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "less": "^2.5.3",
    "less-plugin-clean-css": "^1.5.1",
    "mdast": "^2.1.0",
    "mdast-lint": "^1.1.1"
  },
  "mdastConfig": {
    "plugins": {
      "lint": {
        "emphasis-marker": "*",
        "list-item-indent": "space",
        "maximum-line-length": 120,
        "no-html": false,
        "strong-marker": "*"
      }
    },
    "settings": {
      "commonmark": true,
      "yaml": true,
      "footnotes": true
    },
    "output": false
  }
}